= button_tag 'Add a Note', class: "tiny button radius add-note"
= form_for [@feedback,Note.new], html: { class: 'note-form' } do |f|
  = f.label :item
  = f.text_field :item

  = f.label :rating
  = f.text_field :rating
  %h6.subheader Rate from 1 - 5 scale

  = f.submit :SaveNote , class: 'btn btn-success save-button'
  = button_tag 'Cancel', class: "btn btn-danger cancel-button"
